<p id="notice"><%= notice %></p>

<h1>Strategies</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Description</th>
      <th>Agent</th>
      <th>Map</th>
      <th>Side</th>
      <th>Difficulty</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @strategies.each do |strategy| %>
      <tr>
        <td><%= strategy.title %></td>
        <td><%= strategy.description %></td>
        <td><%= strategy.agent %></td>
        <td><%= strategy.map %></td>
        <td><%= strategy.side %></td>
        <td><%= strategy.difficulty %></td>
        <td><%= link_to 'Show', strategy %></td>
        <td><%= link_to 'Edit', edit_strategy_path(strategy) %></td>
        <td><%= link_to 'Destroy', strategy, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Strategy', new_strategy_path %>
